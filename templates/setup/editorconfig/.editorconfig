---
id: editorconfig-standard
version: 1.0.0
category: setup
description: Standard .editorconfig for consistent coding styles
language: ini
dependencies: []
variables: []
---

# EditorConfig
# Generated by Project Catalyst
# https://editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.{yml,yaml}]
indent_size = 2

[*.py]
indent_size = 4

[*.{java,kt}]
indent_size = 4

[Makefile]
indent_style = tab

[*.go]
indent_style = tab
